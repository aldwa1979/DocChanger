@model IEnumerable<DocChanger.Models.ImportModel>


@{
    ViewData["Title"] = "List";
}

<h1>List</h1>

<a style="margin-right: 10px;" href="@Url.Action("Raport", "Home")" role="button" class="btn btn-primary pull-right">
    <span class="glyphicon glyphicon-cloud-download"></span>
    Raport MT103
</a>


<table class="table table-striped table-hover">
    <thead>
        <tr>
            <th>Destynacja</th>
            <th>Kod Hotelu</th>
            <th>Nazwa Hotelu</th>
            <th>Kwota</th>
            <th>Data</th>
            <th>Nazwa</th>
            <th>Adres</th>
            <th>Kraj</th>
            <th>Iban</th>
            <th>SWIFT</th>
            <th>Waluta</th>
            <th>Tytuł przelew</th>
            <th>Rozliczenie prowizji</th>
            <th>Rachunek zleceniodawcy</th>
            <th>Rachunek dla opłat</th>
            <th>Kategoria biznesowa</th>
            <th>Tryb realizacji</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
        <tr>
            <td>
                @item.Dest
            </td>
            <td>
                @item.HotelCode
            </td>
            <td>
                @item.Hotel
            </td>
            <td>
                @item.Amount
            </td>
            <td>
                @item.Date.ToShortDateString()
            </td>
            <td>
                @item.Name
            </td>
            <td>
                @item.Address
            </td>
            <td>
                @item.Country
            </td>
            <td>
                @item.IBAN
            </td>
            <td>
                @item.SWIFT
            </td>
            <td>
                @item.Currency
            </td>
            <td>
                @item.Title
            </td>
            <td>
                @item.Commission
            </td>
            <td>
                @item.GrecosBank1
            </td>
            <td>
                @item.GrecosBank2
            </td>
            <td>
                @item.Category
            </td>
            <td>
                @item.Realisation
            </td>
        </tr>
        }
    </tbody>
</table>